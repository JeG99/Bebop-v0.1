routine find;


main{

    arr: int[10];
    count: int;
    ans: int;
    
    count = 0;
    ans = -1;
    arr[0] = 4;
    arr[1] = 5;
    arr[2] = 6;
    arr[3] = 8;
    arr[4] = 3;
    arr[5] = 7;
    arr[6] = 1;
    arr[7] = 2;
    arr[8] = 0;
    arr[9] = 10;
    arr[10] = 9;
    in: int;
    <<<("Ingrese el numero que desea buscar");
    >>>in;
    <<<("---");
    while(count < 11){
        if(arr[count] == in){
            ans = count;
        }
        count = count + 1;
    }
    if(ans == -1){
        <<<("No se encontro");
    }
    else{
        <<<("La respuesta es:",ans);
    }
}