routine arrOperations;

main{
    arr: int[10];
    count: int;
    count = 0;
    arr[3 + 2] = 5;
    <<<(arr[5]);
    while(count < 11){
        arr[count] = count * 2;
        count = count + 1;
    }
    count = 0;
    while(count < 6){
        <<<(arr[count + 1]);
        count = count + 1;
    }
}