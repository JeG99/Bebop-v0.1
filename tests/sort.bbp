routine sort;

main{

    arr: int[10];
    cont1: int;
    cont2: int;
    temp : int;
    
    
    arr[0] = 4;
    arr[1] = 5;
    arr[2] = 6;
    arr[3] = 8;
    arr[4] = 3;
    arr[5] = 7;
    arr[6] = 1;
    arr[7] = 2;
    arr[8] = 0;
    arr[9] = 10;
    arr[10] = 9;

    cont1 = 0;
    cont2 = 0;
    
    while(cont1 < 11){
        cont2 = cont1;
        while(cont2 < 10){
            if(arr[cont1] < arr[cont2 + 1]){
                temp = arr[cont1];
                arr[cont1] = arr[cont2+1];
                arr[cont2+1] = temp;
            }
            cont2 = cont2 + 1;
        }
        
        cont1 = cont1 + 1;
    }
    cont1 = 0;
    while(cont1 < 11){
        <<<(arr[cont1]);
        cont1 = cont1 + 1;
    }
}